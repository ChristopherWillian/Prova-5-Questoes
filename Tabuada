#include <stdio.h>


int a, b;
 int
main () 
{
  
printf ("Digite um número:");
  
scanf ("%d", &a);
  
printf ("Tabuada do 1 até %d\n", a);

for (int e = 1; e <= a; e++)
    {
      
for (int i = 1; i <= 10; i++)
	{
	  
b = e * i;
	  
printf ("%d x %d = %d\n", e, i, b);
	  
if (i == 10)
	    {
	    
 
}
	
}
    
}
  
 
 
return 0;

}
